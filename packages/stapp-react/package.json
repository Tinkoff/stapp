{
  "name": "stapp-react",
  "version": "2.3.3",
  "description": "React bindings for stapp - modular state manager",
  "keywords": [
    "state-management",
    "stapp",
    "state",
    "reactive",
    "microservice",
    "epics",
    "redux",
    "react"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "author": "Dmitry Korolev <dmitry@korolev.dk> (https://korolev.dk)",
  "repository": {
    "type": "git",
    "url": "https://github.com/TinkoffCreditSystems/stapp/"
  },
  "homepage": "https://stapp.js.org/guides/react.html",
  "bugs": "https://github.com/TinkoffCreditSystems/stapp/issues?q=is:issue+label:stapp-react",
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run build:module",
    "build:module": "tsc",
    "prebuild": "rimraf lib",
    "test": "npm run test:lint && npm run test:jest",
    "test:lint": "tslint -t verbose './src/**/*.ts' './src/**/*.tsx' -p ./tsconfig.json -c ./tslint.json",
    "test:jest": "jest --config ./jest.config.json",
    "test:ci": "npm run test:lint && jest --config ./jest.config.json --runInBand --coverage --coverageReporters=text-lcov | coveralls"
  },
  "peerDependencies": {
    "fbjs": ">=0.8",
    "prop-types": ">=15.6",
    "react": ">=15",
    "stapp": ">=2.0.0",
    "stapp-formbase": ">=2.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.0",
    "@types/node": "^10.3.3",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.3.6",
    "@types/react-dom": "^16.0.4",
    "babel-jest": "^23.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.4.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "fbjs": "^0.8.16",
    "jest": "^23.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "redux": "^4.0.0",
    "request": "^2.85.0",
    "reselect": "^3.0.1",
    "stapp": "^2.3.0",
    "stapp-formbase": "^2.3.0",
    "ts-jest": "^23.0.1",
    "ts-node": "^7.0.0",
    "tslib": "^1.9.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-react": "^3.5.1",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/create-subscription": "^16.4.1",
    "create-subscription": "^16.4.2",
    "light-observable": "^2.12.0"
